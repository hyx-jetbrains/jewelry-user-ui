page{
	width: 100%;
	height: 100%;
}
/**index.wxss**/
.page-top{
	width: 100%;
	height: 20%;
	border-bottom: 2rpx solid #F5F5F5;
	overflow:scroll;
 }
.containere{
   z-index: 1;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/*搜索框*/
.searchs{
	width: 100%;
	height: 40%;
	padding-top: 5%;
	background-color: #F5F4F5;
}
.searches{
	width: 90%;
	height: 100%;
    margin-left: 5%;
   /* margin-bottom: 3%;*/
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	background-color: #FFFFFF;
	border-radius: 55rpx;
}

.explores{
	width: 100%;
	height: 60rpx;
}

.list-top{
	width: 96%;
	height: auto;
	margin-left: 2%;
	padding-top: 3%;
	/*margin-top: 3%;*/
	/*margin-bottom: 4%;*/
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.list-top text{
	width: 80%;
}

/*列表标题*/
.item{
	color: #333333;
}
.list-top .item.active{  
  color: #C89F5F;
  border-bottom: 1rpx solid #C89F5F;
  padding-bottom: 5rpx;
  height: 100%;
}

.containers{
	width: 100%;
	height: auto;
	display:block;
}
swiper{
	width: 100%;
}
block swiper-item {
  width: 100%;
  height: 100%;
}
.slide-image{
	width: 100%;
	height: 100%;
}

/*产品列表*/

.ser-list{
	width: 100%;
	height: auto;
	display: flex;
	flex-direction: column;
}
.ngv-list{
	width: 100%;
	margin-top: 2%;
}
.titles{
	width: 100%;
	height: auto;
	padding-top: 3%;
	margin-top: 3%;
	display: flex;
	flex-direction: row;
	justify-content: center;
}

.img-title{
	width: 100%;
	height:auto;
	display: flex;
	border: 2rpx solid red;
}
.img-title image{
	width: 100%;
	border: 2rpx solid blue;
}

.lefts{
	width: 25%;
	height: auto;
	margin-top: 25rpx;
	margin-right: 5%;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.leftLine{
	width: 90%;
	height: auto;
	margin-top: 3rpx;
	height: 3rpx;
	background-color:#C89F5F;

}
.lefts image{
	width: 12rpx;
	height: 12rpx;
}
.leftss{
	width: 25%;
	height: auto;
	margin-top: 25rpx;
	margin-left: 5%;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.leftLines{
	width: 90%;
	height: auto;
	margin-top: 5rpx;
	height: 3rpx;
	background-color:#C89F5F;

}
.leftss image{
	width: 12rpx;
	height: 12rpx;
}

/*副标题*/
.subtitle{
	width: 100%;
	height: auto;
	color: #999999;
	font-size: 25rpx;
	display: flex;
	justify-content: center;
	margin-bottom: 3%;
}

.big-title{
	display: flex;
	color: #C89F5F;
}
/*滚动x轴*/
.scrollsx{
	white-space: nowrap; 
	display: flex;
	width: 100%;
	height: auto;
}


.small-title{
	display: flex;
}
/*大图上的商品列表*/
.list-ss{
	display: flex;
	flex-direction: row;
}

.ser-lists{
	width:  100%;
	height: auto;
	display: flex;
	background-color: #F5F5F5;
	flex-wrap: wrap;
	
}

.ser-lists blcok{
	width: 30%;
	height: auto;
	margin-top: 2%;
	margin-left: 2.5%;
	margin-bottom: 2%;
	display: flex;
	background-color: #FFFFFF;	
}
.goods navigatorhover{
	display: flex;
	widows: 100%;
}
.goods{
	width: 100%;
	height:auto;
	margin: 3%;
	display: flex;
	/*border: 1px solid #EEEFF0;*/
}

.goods navigator{
	width: 100%;
	height: auto;
}
.img{
	width: 100%;
	height: auto;
}
.img-one{
	width: 100%;
	height: 100%;	
}
.title{
	width: 100%;
	height: auto;
	color: #A1A1A1;
	font-size: 30rpx;
}
.price{
	width: 100%;
	height: auto;
	color: #C7A36E;
	/*padding-left: 10%;*/
	font-size: 30rpx;
}




/*二级列表样式*/
.xilitLists{
	width: 100%;
	height: auto;
	margin-top: 5%;
	margin-bottom: 5%;
	display: flex;
	justify-content: center;
	align-items: center;
}
.xilielists{
	width: 90%;
	height: auto;
	display: flex;
	justify-content: space-between;
	flex-direction: row;
}
.xilielists block{
	width: 15%;
	height: auto;
	display: flex;
	flex-direction: column;
	
}
.listimgtext{
	width: 100%;
	height: auto;
	display: flex;
	
}

.listimgtext navigator{
	width: 100%;
	height: auto;
	display: flex;
	flex-direction: column;
}

.listimgtext image{
	width: 100%;
	height: 70rpx;
	display: flex;
}
.tilexx{
	width: 100%;
	height: auto;
	margin-top: 4%;
	color: #666666;
	display: flex;
    justify-content: center;
    font-size: 30rpx;
}


/*图片*/
.imgess{
   width: 100%;
   height: auto;
   margin-bottom: 2%;
   margin-top: 2%;
}
