page{
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
	z-index: 13;
}

.page-top{
	width: 100%;
	height: auto;
	z-index: 12;
	border-bottom: 2rpx solid #F5F5F5;
 }

.containere{
	width: 100%;
    z-index: 1;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/*搜索框*/
.searchs{
	width: 100%;
	height: 100%;
	background-color: #F5F4F5;
}
.searches{
	width: 90%;
	height: 100%;
    margin-left: 5%;
    margin-bottom: 3%;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	background-color: #FFFFFF;
	border-radius: 55rpx;
}
.explore{
	width: 50rpx;
    height: 50rpx;
    display: flex;
    padding-top: 2%;
    padding-bottom: 2%;

}
.inputs{
	width: 90%;
	height: auto;
	display: flex;
	color: #DFE0E1;
	background-color: #FFFFFF;
}

/*列表标题*/


.list-top{
	width: 96%;
	height: auto;
	margin-left: 2%;
	margin-top: 3%;
	margin-bottom: 4%;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.list-top text{
	width: 80%;
}

.item{
	color: #333333;
}
.list-top .item.active{  
  color: #C89F5F;
  border-bottom: 1rpx solid #C89F5F;
  padding-bottom: 5rpx;
  height: 100%;
}

.containers{
	width: 100%;
}

.containere{
	width: 100%;
}

/*产品列表*/
.ser-list{
	width: 100%;
	height: auto;
	display: flex;
	background-color: #FFFFFF;
	flex-wrap: wrap;
}

.ser-list blcok{
	width: 50%;
	height: auto;
	display: flex;
	background-color: #FFFFFF;
}
.goods{
	width: 100%;
	height:auto;
	padding: 3%;
	display: flex;
	/*border: 1px solid #EEEFF0;*/
}

.goods navigator{
	width: 100%;
	height: auto;
}


.img{
	width: 100%;
	height: auto;
}
.img-one{
	width: 100%;
	height: 120rpx;	
}
.title{
	width: 100%;
	height: auto;
	
}
.introduction{
	font-size: 25rpx;
	color: #A1A1A1;
	margin-bottom: 2%;
}
.price{
	width: 100%;
	height: auto;
	color: #C7A36E;
}

